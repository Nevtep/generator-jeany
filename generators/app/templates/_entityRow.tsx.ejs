import * as React from 'react';
import { DataTable } from 'react-native-paper';
import { <%= queryName.split('_').map(part => part.substr(0,1) + part.substr(1).toLowerCase()).join('') %>_<%= entityName.toLowerCase()%>s } from '../queries/__generated__/<%= queryName.split('_').map(part => part.substr(0,1) + part.substr(1).toLowerCase()).join('') %>';

export interface <%= entityName %>RowProps extends Partial<<%= queryName.split('_').map(part => part.substr(0,1) + part.substr(1).toLowerCase()).join('') %>_<%= entityName.toLowerCase()%>s> {
}

export default class <%= entityName %>Row extends React.Component<<%= entityName %>RowProps> {
  render() {

    return (

        <DataTable.Row>
            <% for (property of props) { %>
                <DataTable.Cell<%= property.isNumeric ? ' numeric' : ''%>>{this.props.<%= property.name %>}</DataTable.Cell>
            <% } %>
        </DataTable.Row>

    );
  }
}