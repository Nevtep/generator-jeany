import gql from 'graphql-tag';

export const <%= queryName %> = gql`
query <%= queryName.split('_').map(part => part.substr(0,1) + part.substr(1).toLowerCase()).join('') %> {
    <%= queryEntity.toLowerCase() %>s() {
        <% for (property of props) {%>
            <%= property.name %>
        <% } %>
    }
}
`